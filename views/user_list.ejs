<%- include('partials/header', { pageTitle: 'Users - ' + shopData.shopName }) %>

    <h2>Here are the users:</h2>
    <!-- List of available users -->
    <ul>
        <% users.forEach(user=> { %>
            <li>
                <%= user.username %> - <%= user.first_name %>
                        <%= user.last_name %>, Email: <%= user.email %> <a
                                    href="/users/delete/<%= user.id %>">Delete</a>
            </li>
            <% }); %>
    </ul>
    <a href="/">Back</a>

    <%- include('partials/footer') %>